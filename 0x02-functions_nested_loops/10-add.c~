#include "main.h"
/**
* main - adds two integers and returns the result
*
* Result: 0 Always
*/
int main(void)
{
int add(int x, int y);
return (0);
}
/**
* add - adds two integers and returns the result
*
* @x:integer to add
* @y:integer to add
*
* Return: result
*/
int add(int x, int y)
{
int result;
result = x + y;
return (result);
}
